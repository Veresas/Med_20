<Page x:Class="Med_20.Pages.PageMain"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Med_20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageMain">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="131*"/>
            <ColumnDefinition Width="669*"/>
        </Grid.ColumnDefinitions>
        <StackPanel >
            <Button x:Name="but_enter" Click="but_enter_Click" Height="30" Margin="0,0,0,10" > Авторизация</Button>
            <Button x:Name="but_rep" Height="30" Margin="0,0,0,10" >Отчеты</Button>
            <Button x:Name="but_aplication" Height="30" Margin="0,0,0,10" >Заявки</Button>
            <Button x:Name="but_reserch" Height="30" Margin="0,0,0,10" >Исследования</Button>
            <Button x:Name="but_bill" Height="30" Margin="0,0,0,10" >Счета</Button>
            <Button x:Name="but_historyEnter" Click="but_historyEnter_Click" Height="30" Margin="0,0,0,10" >История посещения</Button>
            <Button x:Name="but_resources" Height="30" Margin="0,0,0,10" > Ресурсы</Button>
        </StackPanel>
        <Frame x:Name="SupFrame" NavigationUIVisibility="Hidden" Grid.Column="1"></Frame>
    </Grid>
</Page>
