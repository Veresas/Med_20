<Page x:Class="Med_20.Pages.PageHisrtoryEnter"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Med_20.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageHisrtoryEnter">

    <Grid>
        <DataGrid x:Name="DG_history_enter" AutoGenerateColumns="False" >
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Users.login}" Header="Логин"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding Date_of_enter}" Header="Дата входа"></DataGridTextColumn>
                <DataGridCheckBoxColumn Binding="{Binding IsSuccess}" Header="Успешно"></DataGridCheckBoxColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
